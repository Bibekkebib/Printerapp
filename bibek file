public class printerapp
 { public static void main(String[] args) throws Exception { 
    System.out.println("This app is made by:");

  
  String creator = "Bibek Pandey";
  System.out.println("This app is made by:" + creator);

  int number1 = 5;
  int number2 = 2;

 System.out.println("The value of number1 is: " + number1);
 System.out.println("The value of number2 is: " + number2);

 int multiplication = number1*number2;
 System.out.println("5 * 2 = " + multiplication);


 int Addition = number1+number2;
 System.out.println("5 + 2 = "+ Addition);

 double Division = number1/number2;
 System.out.println("5 / 2 = "+ Division);
 
 int Subtraction = number1-number2;
 System.out.println("5 - 2 = "+ Subtraction); 

 
 }
}
